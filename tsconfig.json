{
    "compilerOptions": {
      "target": "esnext",
      "lib": ["es2022"],
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noImplicitAny": false,
      "types": ["node"],
      "strict": true,
      "noUnusedLocals": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src"]
}